#gallery {background-color: #191919;}
    #gallery h2 {color: #ffffff; text-align: center; padding-top: 40px;}
    #gallery article {display: table-cell; vertical-align: top;}
    #gallery hr {width: 50px; height: 5px; margin: 10px auto 30px; background-color: rgba(249,235,27,1); border-color: rgba(249,235,27,1);}
        .albswitcher {list-style: none; padding: 0px; margin: 20px auto; text-align: center;}
            .albswitcher li.alb {display: inline-block; color: #ffffff; font-family: pfebold; font-size: 14px; text-transform: uppercase; cursor: pointer; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
            .albswitcher li.alb:hover, .albswitcher li.activalb {color: #f9eb1b;}
            .albswitcher li.alb:before, .albswitcher li.alb:last-child:after {content: ''; display: inline-block; width: 60px; height: 1px; background-color: #2c2c2c; margin: 0px 10px 6px;}
            div.alb {display: none; width: 100%; line-height: 1px; overflow: hidden; -webkit-transition: all 1s;  -moz-transition: all 1s; -o-transition: all 1s; transition: all 1s;}
            div#alb1 {display: block;
                      margin-top: 50px;}
                div.alb img {width: 25%; vertical-align: top; margin: 0; padding: 0px; border: 0px; opacity: 0.4; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                div.alb img:hover {opacity: 1;}
            #gallery article .show_wbt {display: none; visibility: hidden;}
            #gallery a{
                margin-top: 50px;
                margin-bottom: 50px;
            }

#prev {display: none; width: 100%; height: 100%; background-color: #000000; position: fixed; top: 0; left: 0; z-index: 999992;}
    .imgblock {width: 1000px; height: 100%; margin: 0px auto; z-index: 9999999;}
        .imgblock a {display: block; height: 66px; width: 34px; position: absolute; top: 50%;  margin-top: -33px; opacity: 0.5; cursor: pointer; background-repeat: no-repeat; background-position: center center; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
        .imgblock a:hover {opacity: 1;}
        #back {left: 50%; margin-left: -500px; background-image: url('img/before.png');}
        #next {right: 50%; margin-right: -500px; background-image: url('img/next.png');}
        #close {height: 32px; width: 32px; top: 0; margin-top: 20px; right: 50%; margin-right: -500px; background-image: url('img/close.png');}
        #prev img {display: block; max-width: 900px; height: auto; margin: 0 auto; }

#widget {display: none; width: 100%; height: 100%; background-color: rgba(0,0,0,0.7); position: fixed; top: 0; left: 0; z-index: 999992;}
    .widgetform {display: block; width: 700px; height: 300px; margin: 0px auto; z-index: 9999999; background-color: rgba(255,255,255,1);}
    #widget .yellow-btn {display: block; position: relative; z-index: 999; width: 300px; margin: -100px auto 0px; text-align: center; line-height: 30px;}

