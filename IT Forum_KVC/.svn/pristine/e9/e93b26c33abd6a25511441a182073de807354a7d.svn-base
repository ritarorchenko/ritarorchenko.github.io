@font-face {
    font-family: intro;
    src: local("intro"),
         url(fonts/Intro.otf);
}
@font-face {
    font-family: pfethin;
    src: local("PFEncoreSansPro-Thin"),
         url(fonts/PFEncoreSansPro-Thin.otf);
}
@font-face {
    font-family: pfelight;
    src: local("PFEncoreSansPro-Light"),
         url(fonts/PFEncoreSansPro-Light.otf);
}
@font-face {
    font-family: pfemedium;
    src: local("PFEncoreSansPro-Medium"),
         url(fonts/PFEncoreSansPro-Medium.otf);
}
@font-face {
    font-family: pfebold;
    src: local("PFEncoreSansPro-Bold"),
         url(fonts/PFEncoreSansPro-Bold.otf);
}
@media screen and (max-width: 1280px) { body {width: 100% !important;} }
@media screen and (max-width: 1140px) { body {width: 100% !important;} }
@media screen and (max-width: 992px) { body {width: 970px !important;} }

*, *:before, *:after { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;}

html, body {width: 100%; margin: 0px; padding: 0px; height: 100%;}
.clr {clear: both;}

h2 {font-family: intro; font-size: 40px; margin: 0px;}
strong {font-family: pfebold;}

.content {width: 100%; background-position: top center; background-repeat: no-repeat; background-size: 100%;}
.fh {height: 100%; min-height: 100%; background-size: 100% 100%; display: table;}
    .fh article {display: table-cell; vertical-align: middle;}
.yellow-btn {display: block; color: #faeb1b; text-decoration: none; font-family: intro; border: 3px solid #faeb1b; cursor: pointer; -webkit-transition: all 0.5s; -moz-transition: all 0.5s; -o-transition: all 0.5s; transition: all 0.5s;}
.yellow-btn:hover {background-color: #faeb1b; color: #222222;}

#screen1 {background-image: url(img/cover1.jpg); background-attachment: local; background-size: cover; min-height: 100%;}
    #screen1 article {width: 600px; padding-left: 10px 0px; text-align: center;}
@media screen and (max-width: 1280px) {#screen1 article{width: 1200px !important;} }
@media screen and (max-width: 1140px) {#screen1 article{width: 1024px !important;} }
@media screen and (max-width: 992px) {#screen1 article{width: 970px !important;} }

    #screen1 h1 {font-family: intro; font-size: 95px; color: #ffffff; margin: 35px 0px;}
    #screen1 p {margin: 0px auto 15px; width: auto; color: #ffffff; font-size: 22px; font-family: pfethin;}
        #screen1 p img {margin: 2px 7px 0 0;}
    #screen1 a {text-align: center; padding: 20px 0px 15px;}
    #screen1 .button{
        width: 250px;
        margin-top: 55px;
        text-align: center;
    }
    #screen .button a{
        border-radius: 8px;
    }


#menu {width: 100%; background-color: #111111; font-size: 13px; height: 65px;}
#menu nav {width: 640px; margin: 0 auto;}
@media (min-width: 940px) {
    #menu nav {width: 800px;
    }
}
.fixed {position: fixed; top:0px; z-index: 99999;}
        .menu, .lang {width: 640px; margin: 0 auto; padding: 0px; list-style: none;  display: block;}
            .menu li {display: inline-block; color: #afafaf;}
                .menu li a {display: block; color: #afafaf; text-decoration: none; padding: 25px 13px 13px; font-family: pfemedium; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                .menu li:first-child a {padding-left: 0px;}
                    .menu li a hr {width: 22px; height: 3px; margin-bottom: 0px; background-color: rgba(249,235,27,0); border-color: rgba(249,235,27,0); -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                .menu li a:hover, ul.menu li.active a {color: #faeb1b; cursor: pointer;}
                    .menu li a:hover hr, ul.menu li.active a hr {background-color: rgba(249,235,27,1);}
    nav a.yellow-btn {padding: 10px 0px 7px; width: 147px; text-align: center; float: left; margin: 12px 30px 0px;}
            .lang li {display: inline-block;}
                .lang li a {display: block; color: #afafaf; text-decoration: none; padding: 25px 10px; margin-left: 5px; font-family: pfelight; -webkit-transition: all 0.5s; -moz-transition: all 0.5s; -o-transition: all 0.5s; transition: all 0.5s;}
                .lang li a:hover, .lang li a.cur-lang  {color: #ffffff;}

#about {background-image: url('img/screen2.png'); background-size: 100%; background-repeat: repeat; color: #444444; min-height: 460px;}
.fix-menu {margin-top: 65px;}
    #about article {width: 100%; height: 455px; text-align: center;}
        #about h2 {padding-top: 70px;}
        #about article hr {width: 50px; height: 5px; margin: 10px auto 30px; background-color: rgba(249,235,27,1); border-color: rgba(249,235,27,1);}
        #about article p {font-family: pfelight; font-size: 15px; line-height: 20px;margin: 0px 0px 20px;}
            .willbe {margin: 0 auto; text-align: center; font-size: 14px; color: #444444; padding-bottom: 20px;}
            @media (min-width: 940px) {
                .willbe{width: 940px;}
            }
                .willbe div {vertical-align: top; width: 280px; display: inline-block; margin: 0px 8px 25px; /*vertical-align: top;*/}
                    .willbe .icon {height: 90px; margin-top: 10px;}
                    .willbe h5 {text-transform: uppercase; font-family: pfebold;}
                    .willbe span {font-family: pfelight;}

#screen7 {background-image: url(img/screen7.png); background-attachment: fixed; background-size: cover; background-color: rgb(105, 81, 47); color: #ffffff; font-family: pfelight; text-align: center;}
#screen7 h2 {color: #faeb1b; font-family: pfelight; font-size: 36px; padding-top: 50px; width: 640px; margin: 0px auto; text-align: left;}
    #screen7 p {font-size: 15px; width: 660px; margin: 0 auto; padding-left: 10px; padding-bottom: 60px; text-align: left; line-height: 30px;}
        #screen7 p img {margin-right: 15px; vertical-align: middle;}
@media (min-width: 940px) {
    #screen7 .content{width: 940px;}
}
#program {background-size: auto; background-color: rgba(248,204,9,1); background: linear-gradient(to bottom, #f4df02, #dc7607);}
    #program article {background-image: url('img/screen3.png'); background-size: auto; background-repeat: repeat; background-attachment: fixed; max-width: 1000px; -webkit-transition: all 2s; -moz-transition: all 2s; -o-transition: all 2s; transition: all 2s;}
    .programhidden {display: none !important;}
        .topscreen {width: 65%; z-index: 999;float: left;}
        .top-s-f {position: fixed !important; top: 0px !important; left: 0px !important;}
            .pro-title {width: 46%; margin-left: 54%; height: 136px;}
                .pro-title hr {width: 50px; height: 5px; margin: 10px auto 0; background-color: rgba(255,255,255,1); border-color: rgba(255,255,255,1);}
                .pro-title1 h2 {text-align: center; padding-top: 70px;}
                .pro-title1 h3 {text-align: center; padding-top: 10px; font-family: pfethin; font-weight: bolder}
                .pro-title1 hr {width: 50px; height: 5px; margin: 10px auto 0; background-color: rgba(255,255,255,1); border-color: rgba(255,255,255,1);}
                .timer {width: 46%; float: left; text-align: center; height: 100%; background-image: url('img/verticaldots.png'); background-position: center center; background-repeat: no-repeat; background-size: auto;}
                    .timerdisplay { width: 150px; height: 150px; margin: 0px auto; border: 2px solid #ffffff; border-radius: 75px; font-family: pfethin; font-size: 30px; line-height: 30px}
                        .timerdisplay pre {font-family: pfethin;}
            .prog_battons {width: 46%; margin-left: 54%; height: 136px; text-align: center;}
                .prog_battons a, #program2 a, #gallery a {text-decoration: none; text-align: center; padding-top: 15px; padding-bottom: 15px; font-size: 14px; display: block; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                .show {font-family: pfemedium; text-transform: uppercase; cursor: pointer; color: #000000; line-height: 16px;}
                .show:hover {color: #ffffff;}
                .whitebtn {font-family: intro; font-size: 14px; text-transform: uppercase; color: #ffffff; cursor: pointer; line-height: 1; vertical-align: middle; border: 2px solid #ffffff; width: 250px; margin: 15px auto; padding: 10px 0 10px;}
                .whitebtn:hover {color: #000000; background-color: #ffffff;}
                .down {width: 39px; height: 39px; margin: 10px auto; background-image: url('img/nextslide.png'); -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                .down:hover {background-image: url('img/nextslidehover.png');}
        .timetable {width: 35%; height: auto; float: right; font-family: pfethin;}
            .timetable .tt-list {list-style: none; padding: 0px; width: 71%; float: left; margin: 0px;}
                .timetable .tt-list li {display: block; height: 150px; font-size: 15px; font-family: pfelight;}
                    .timetable h5 {font-size: 30px; margin-bottom: 0px;}
            .tt-switcher {width: 100px; float: right; padding: 0px; margin: 0px; list-style: none; right: 0;}
                .tt-switcher li {vertical-align: middle;}
                    .tt-switcher a {display: block; width: 12px; height: 12px; margin: 10px auto; border: 0px solid rgba(255,255,255,0); border-radius: 8px; background-color: rgba(255,255,255,1); -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                    .tt-switcher a:hover, .activeTT a {border: 2px solid rgba(255,255,255,1); border-radius: 6px; background-color: rgba(255,255,255,0);}
    .background2 {background-color: rgba(248,204,9,1); background: linear-gradient(to bottom, #f4df02, #f9c40c) !important;}
    .background2 article {vertical-align: top;}
        .background2 h2 {text-align: center; padding-top: 90px;}
        .background2 hr {width: 50px; height: 5px; margin: 10px auto 0; background-color: rgba(255,255,255,1); border-color: rgba(255,255,255,1);}
        #program2 {margin: 40px auto;}
            #program2 ul {display: block; width: 320px; float: left; list-style: none; padding: 0px; margin: 0px;}
                #program2 ul li {display: block; width: 290px; height: 65px; margin: 20px 0px; cursor: pointer; font-family: pfebold; font-size: 14px; text-transform: uppercase; line-height: 65px; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                    #program2 ul li span {display: block; margin: 0px; width: 250px; float: left; background-color: rgba(0,0,0,0.07); text-align: center; color: #ffffff; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                    #program2 ul li:hover span, #program2 ul li.nomactiv span {color: #444444; background-color: #ffffff;}
                #program2 ul li:after {content: ''; display: inline-block; width: 20px; height: 65px; float: right; border-left: 20px solid rgba(255,255,255,0); border-top: 32px solid rgba(0,0,0,0); border-bottom: 32px solid rgba(0,0,0,0); border-right: 20px solid rgba(0,0,0,0); -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                #program2 ul li:hover:after, #program2 ul li.nomactiv:after { border-left: 20px solid rgba(255,255,255,1);}
            @media (min-width: 940px) {
                #program2 table { width: 800px;
                }
            }
            #program2 table {display: none; color: #000000; font-family: pfelight; vertical-align: top; font-size: 24px; font-family: pfelight;}
            #program2 table#progtab1 {display: block; margin: 0 auto;}
                    #program2 table tr td {padding-bottom: 26px; padding-top: 0px;  vertical-align: top; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                    .active  {padding-bottom: 26px; padding-top: 0px; cursor: pointer; vertical-align: top; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}

                    #program2 table tr td:first-child {width: 175px; text-align: right;}
                    #program2 table tr td:nth-child(2) {width: 50px; background-image: url('img/vpoint.png'); background-position: top center; background-repeat: repeat-y; background-size: auto;}
                        #program2 table tr td:nth-child(2) span {display: block; width: 20px; height: 20px; background-color: #ffffff; border: 2px solid #ffffff; border-radius: 10px; margin: 4px auto; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                        #program2 table tr:last-child td:nth-child(2) {background-image: none;}
                        #program2 table tr:hover td:nth-child(2) span, #program2 table tr.opened td:nth-child(2) span {background-color: #f6d306;}
                    #program2 table tr td:last-child p {color: #000000; font-size: 15px; line-height: 25px;}
                        #program2 td b {display: inline-block; width: 13px; height: 7px; background-image: url('img/down.png'); margin-left: 10px;  display: none; -webkit-transition: all 0.3s; -moz-transition: all 0.3s; -o-transition: all 0.3s; transition: all 0.3s;}
                        #program2 tr:hover td b {background-image: url('img/downh.png');}
                        #program2 .opened td b {background-image: url('img/downh.png'); transform: rotate(180deg);}
                        .where {color: #835320;}
                    #program2 table .active:hover td, .opened {color: #ffffff !important;}

#screen4 {background-image: url(img/screen4.png); background-attachment: fixed; background-size: cover; background-color: darkslategrey; height: 340px; color: #ffffff; font-family: pfelight; text-align: center;}
